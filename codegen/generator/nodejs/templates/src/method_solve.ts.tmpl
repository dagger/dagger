{{ define "method_solve" }}
{{- template "method_comment" . }}
{{- "" }}  async {{ .Name -}}({{- template "args" .Args -}}){{ template "return_solve" .TypeRef }} {
		{{- if .TypeRef }}
    this._queryTree = [
      ...this._queryTree,
      {
      operation: '{{ .Name }}'
			{{- if .Args }},
      args: { {{- template "call_args" .Args -}} }
			{{- end }}
      }
    ]

    const response: Awaited<{{ .TypeRef | FormatOutputType }}> = await this._compute()

    return response
  }
		{{- end }}
{{- end }}
