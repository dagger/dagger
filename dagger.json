{
  "name": "dagger-dev",
  "engineVersion": "v0.19.6",
  "sdk": {
    "source": "go"
  },
  "toolchains": [
    {
      "name": "ci",
      "source": "toolchains/ci"
    },
    {
      "name": "cli",
      "source": "toolchains/cli-dev"
    },
    {
      "name": "engine-dev",
      "source": "toolchains/engine-dev"
    },
    {
      "name": "go-dev",
      "source": "modules/go",
      "arguments": [
        {
          "function": [],
          "name": "source",
          "ignore": [
              "bin",
              ".git",
              "**/node_modules",
              "**/.venv",
              "**/__pycache__",
              "docs/node_modules",
              "sdk/typescript/node_modules",
              "sdk/typescript/dist",
              "sdk/rust/examples/backend/target",
              "sdk/rust/target",
              "sdk/php/vendor"
          ]
        },
        {
          "function": ["lint"],
          "name": "exclude",
          "default": "[\"docs/**\",\"core/integration/**\",\"dagql/idtui/viztest/broken/**\",\"modules/evals/**\",\"**/broken*/**\"]"
        },
        {
          "function": ["checkTidy"],
          "name": "exclude",
          "default": "[\"docs/**\",\"core/integration/**\",\"dagql/idtui/viztest/broken/**\",\"modules/evals/**\",\"**/broken*/**\"]"       }
      ]
    },
    {
      "name": "go-sdk",
      "source": "toolchains/go-sdk-dev"
    },
    {
      "name": "helm",
      "source": "toolchains/helm-dev"
    },
    {
      "name": "php-sdk",
      "source": "toolchains/php-sdk-dev"
    },
    {
      "name": "python-sdk",
      "source": "toolchains/python-sdk-dev"
    },
    {
      "name": "sdks",
      "source": "toolchains/all-sdks"
    },
    {
      "name": "typescript-sdk",
      "source": "toolchains/typescript-sdk-dev"
    }
  ],
  "include": [
    "engine/distconsts/**/*",
    "sdk/typescript/runtime/**/*",
    "go.mod",
    "go.sum",
    "util/parallel"
  ],
  "dependencies": [
    {
      "name": "alpine",
      "source": "modules/alpine"
    },
    {
      "name": "changelog",
      "source": ".changes"
    },
    {
      "name": "codegen",
      "source": "cmd/codegen"
    },
    {
      "name": "dagger-docs",
      "source": "docs"
    },
    {
      "name": "go",
      "source": "modules/go"
    },
    {
      "name": "ps-analyzer",
      "source": "modules/ps-analyzer"
    },
    {
      "name": "shellcheck",
      "source": "modules/shellcheck"
    }
  ],
  "source": ".dagger"
}
