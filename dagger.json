{
  "name": "dagger-dev",
  "engineVersion": "v0.19.10",
  "sdk": {
    "source": "go"
  },
  "toolchains": [
    {
      "name": "changelog",
      "source": "toolchains/changelog"
    },
    {
      "name": "ci",
      "source": "toolchains/ci"
    },
    {
      "name": "cli",
      "source": "toolchains/cli-dev"
    },
    {
      "name": "docs",
      "source": "toolchains/docs-dev"
    },
    {
      "name": "elixir-sdk",
      "source": "toolchains/elixir-sdk-dev"
    },
    {
      "name": "engine-dev",
      "source": "toolchains/engine-dev"
    },
    {
      "name": "go",
      "source": "toolchains/go",
      "customizations": [
        {
          "argument": "source",
          "ignore": [
            "bin",
            ".git",
            "**/node_modules",
            "**/.venv",
            "**/__pycache__",
            "**/sdk/runtime/**",
            "docs/node_modules",
            "sdk/typescript/node_modules",
            "sdk/typescript/dist",
            "sdk/rust/examples/backend/target",
            "sdk/rust/target",
            "sdk/php/vendor",
            "docs/**",
            "dagql/idtui/viztest/broken/**",
            "modules/evals/**",
            "**/broken*/**",
            "core/integration/testdata/checks/hello-with-checks/"
          ]
        }
      ]
    },
    {
      "name": "go-sdk",
      "source": "toolchains/go-sdk-dev"
    },
    {
      "name": "helm",
      "source": "toolchains/helm-dev"
    },
    {
      "name": "installers",
      "source": "toolchains/installers"
    },
    {
      "name": "java-sdk",
      "source": "toolchains/java-sdk-dev"
    },
    {
      "name": "php-sdk",
      "source": "toolchains/php-sdk-dev"
    },
    {
      "name": "python-sdk",
      "source": "toolchains/python-sdk-dev"
    },
    {
      "name": "release",
      "source": "toolchains/release"
    },
    {
      "name": "rust-sdk",
      "source": "toolchains/rust-sdk-dev"
    },
    {
      "name": "sdks",
      "source": "toolchains/all-sdks"
    },
    {
      "name": "security",
      "source": "toolchains/security",
      "customizations": [
        {
          "function": [
            "scanSource"
          ],
          "argument": "source",
          "ignore": [
            "bin",
            ".git",
            "docs",
            "sdk/rust/examples",
            "sdk/rust/crates/dagger-sdk/examples",
            "core/integration/testdata",
            "dagql/idtui/viztest"
          ]
        }
      ]
    },
    {
      "name": "typescript-sdk",
      "source": "toolchains/typescript-sdk-dev"
    },
    {
      "name": "test-split",
      "source": "toolchains/test-split"
    }
  ],
  "include": [
    "sdk/typescript/runtime/**/*",
    "go.mod",
    "go.sum",
    "util/parallel"
  ],
  "dependencies": [
    {
      "name": "changelog",
      "source": "toolchains/changelog"
    },
    {
      "name": "docs",
      "source": "toolchains/docs-dev"
    },
    {
      "name": "helm",
      "source": "toolchains/helm-dev"
    },
    {
      "name": "sdks",
      "source": "toolchains/all-sdks"
    },
    {
      "name": "engine-dev",
      "source": "toolchains/engine-dev"
    },
    {
      "name": "cli",
      "source": "toolchains/cli-dev"
    }
  ],
  "source": ".dagger"
}
