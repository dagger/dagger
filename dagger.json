{
  "name": "dagger-dev",
  "engineVersion": "v0.19.7",
  "sdk": {
    "source": "go"
  },
  "toolchains": [
    {
      "name": "changelog",
      "source": "toolchains/changelog"
    },
    {
      "name": "ci",
      "source": "toolchains/ci"
    },
    {
      "name": "cli",
      "source": "toolchains/cli-dev"
    },
    {
      "name": "docs",
      "source": "toolchains/docs-dev"
    },
    {
      "name": "engine-dev",
      "source": "toolchains/engine-dev"
    },
    {
      "name": "go",
      "source": "modules/go",
      "arguments": [
        {
          "name": "source",
          "ignore": [
            "bin",
            ".git",
            "**/node_modules",
            "**/.venv",
            "**/__pycache__",
            "docs/node_modules",
            "sdk/typescript/node_modules",
            "sdk/typescript/dist",
            "sdk/rust/examples/backend/target",
            "sdk/rust/target",
            "sdk/php/vendor",
            "docs/**",
            "core/integration/**",
            "dagql/idtui/viztest/broken/**",
            "modules/evals/**",
            "**/broken*/**"
          ]
        }
      ]
    },
    {
      "name": "go-sdk",
      "source": "toolchains/go-sdk-dev"
    },
    {
      "name": "helm",
      "source": "toolchains/helm-dev"
    },
    {
      "name": "installers",
      "source": "toolchains/installers"
    },
    {
      "name": "php-sdk",
      "source": "toolchains/php-sdk-dev"
    },
    {
      "name": "python-sdk",
      "source": "toolchains/python-sdk-dev"
    },
    {
      "name": "java-sdk",
      "source": "toolchains/java-sdk-dev"
    },
    {
      "name": "sdks",
      "source": "toolchains/all-sdks"
    },
    {
      "name": "security",
      "source": "toolchains/security",
      "arguments": [
        {
          "function": [
            "scanSource"
          ],
          "name": "source",
          "ignore": [
            "bin",
            ".git",
            "docs",
            "sdk/rust/examples",
            "sdk/rust/crates/dagger-sdk/examples",
            "core/integration/testdata",
            "dagql/idtui/viztest"
          ]
        }
      ]
    },
    {
      "name": "typescript-sdk",
      "source": "toolchains/typescript-sdk-dev"
    }
  ],
  "include": [
    "engine/distconsts/**/*",
    "sdk/typescript/runtime/**/*",
    "go.mod",
    "go.sum",
    "util/parallel"
  ],
  "dependencies": [
    {
      "name": "alpine",
      "source": "modules/alpine"
    },
    {
      "name": "changelog",
      "source": "toolchains/changelog"
    },
    {
      "name": "codegen",
      "source": "cmd/codegen"
    },
    {
      "name": "docs",
      "source": "toolchains/docs-dev"
    },
    {
      "name": "helm",
      "source": "toolchains/helm-dev"
    },
    {
      "name": "ps-analyzer",
      "source": "modules/ps-analyzer"
    },
    {
      "name": "sdks",
      "source": "toolchains/all-sdks"
    },
    {
      "name": "shellcheck",
      "source": "modules/shellcheck"
    }
  ],
  "source": ".dagger"
}
