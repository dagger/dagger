{
  "compilerOptions": {
    "target": "ES2022",
    "moduleResolution": "Node",
    "experimentalDecorators": true,
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true,
    "allowImportingTsExtensions": true,
    "paths": {
      "@dagger.io/dagger": [
        "./sdk/src"
      ],
      "@dagger.io/dagger/telemetry": [
        "./sdk/src/telemetry"
      ]
    }
  }
}