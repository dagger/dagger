{
  "name": "python-sdk-dev",
  "sdk": "python",
  "exclude": [
    "**/.direnv",
    "**/.venv",
    "**/__pycache__",
    "**/.*cache",
    "**/sdk"
  ],
  "dependencies": [
    {
      "name": "dockerd",
      "source": "github.com/kpenfound/dagger-modules/dockerd@57352e06a1cfbcb5307c009d37c0201b2719b935"
    },
    {
      "name": "apko",
      "source": "github.com/vito/daggerverse/apko@dd441abffabb98405c6ba74f9fde6a62ecaea81a"
    }
  ],
  "source": "dev",
  "engineVersion": "4b825dc642cb6eb9a060e54bf8d69288fbee4904",
  "views": [
    {
      "name": "default",
      "patterns": [
        "docs/**/*.py",
        "docs/**/*.rst",
        "src/**/*.py",
        "src/**/py.typed",
        "tests/**/*.py",
        "pyproject.toml",
        "README.md",
        "LICENSE"
      ]
    },
    {
      "name": "lint",
      "patterns": [
        "!**/.direnv",
        "!**/.venv",
        "!**/__pycache__",
        "!**/.ruff_cache",
        "!**/sdk/pyproject.toml",
        "!**/sdk/src",
        "**/.gitignore",
        "**/pyproject.toml",
        "**/.ruff.toml",
        "**/ruff*.toml",
        "**/*.py"
      ]
    }
  ]
}
