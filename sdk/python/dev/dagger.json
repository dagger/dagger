{
  "name": "python-sdk-dev",
  "sdk": "python",
  "exclude": [
    "**/.direnv",
    "**/.venv",
    "**/__pycache__",
    "**/.*cache",
    "**/sdk"
  ],
  "dependencies": [
    {
      "name": "apko",
      "source": "github.com/vito/daggerverse/apko@fee9c69c2213c9411844095c4b22884e2993cdbf"
    },
    {
      "name": "dockerd",
      "source": "github.com/kpenfound/dagger-modules/dockerd@57352e06a1cfbcb5307c009d37c0201b2719b935"
    }
  ],
  "source": ".",
  "engineVersion": "dev-922ddb4de207f4a9fc2ee679c89f8cb299008cf1",
  "views": [
    {
      "name": "default",
      "patterns": [
        "docs/**/*.py",
        "docs/**/*.rst",
        "src/**/*.py",
        "src/**/py.typed",
        "tests/**/*.py",
        "codegen/**/*.py",
        "pyproject.toml",
        "requirements-dev.lock",
        "README.md",
        "LICENSE"
      ]
    },
    {
      "name": "lint",
      "patterns": [
        "!**/.direnv",
        "!**/.venv",
        "!**/__pycache__",
        "!**/.ruff_cache",
        "!**/sdk/pyproject.toml",
        "!**/sdk/src",
        "**/.gitignore",
        "**/pyproject.toml",
        "**/.ruff.toml",
        "**/ruff*.toml",
        "**/*.py"
      ]
    }
  ]
}
