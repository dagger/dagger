{
  "name": "@dagger.io/dagger",
  "version": "0.0.0",
  "author": "hello@dagger.io",
  "license": "Apache-2.0",
  "types": "./dist/src/index.d.ts",
  "type": "module",
  "files": [
    "dist/"
  ],
  "exports": {
    ".": "./dist/src/index.js",
    "./telemetry": "./dist/src/telemetry/index.js"
  },
  "engines": {
    "node": ">=20"
  },
  "main": "dist/src/index.js",
  "dependencies": {
    "@grpc/grpc-js": "^1.14.3",
    "@lifeomic/axios-fetch": "^3.1.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/core": "^2.3.0",
    "@opentelemetry/exporter-jaeger": "^2.3.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.209.0",
    "@opentelemetry/sdk-metrics": "^2.3.0",
    "@opentelemetry/sdk-node": "^0.209.0",
    "@opentelemetry/semantic-conventions": "^1.38.0",
    "adm-zip": "^0.5.16",
    "env-paths": "^3.0.0",
    "execa": "^9.6.1",
    "graphql": "^16.12.0",
    "graphql-request": "^7.4.0",
    "graphql-tag": "^2.12.6",
    "node-color-log": "^13.0.3",
    "node-fetch": "^3.3.2",
    "reflect-metadata": "^0.2.2",
    "tar": "^7.5.2",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "yarn run test:node",
    "test:bun": "bun run --bun mocha",
    "test:node": "mocha",
    "test:generate-scan": "tsx ./introspector/test/testdata/generate_expected_scan.ts",
    "lint": "yarn eslint --max-warnings=0 .",
    "fmt": "yarn eslint --max-warnings=0 --fix .",
    "docs:lint": "cd ../../docs/current_docs && eslint -c ../../sdk/typescript/eslint-docs.config.js --max-warnings=0 ./**/*.ts",
    "docs:fmt": "cd ../../docs/current_docs && eslint --fix -c ../../sdk/typescript/eslint-docs.config.js --max-warnings=0 ./**/*.ts"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.0",
    "@otel-test-runner/mocha-test": "0.3.1",
    "@trivago/prettier-plugin-sort-imports": "^6.0.2",
    "@types/adm-zip": "^0.5.7",
    "@types/mocha": "^10.0.10",
    "@types/node": "~25.0.6",
    "@types/tar": "^6.1.13",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "mocha": "^11.7.5",
    "prettier": "^3.7.4",
    "rollup": "^4.55.1",
    "rollup-plugin-dts": "^6.3.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript-eslint": "^8.52.0"
  }
}
