{
  "name": "@dagger.io/telemetry",
  "version": "0.0.5",
  "author": "@dagger.io",
  "license": "Apache-2.0",
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esnext/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "node": {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.cjs"
      },
      "default": "./dist/esnext/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "bun run build:types && bun run build:js",
    "build:types": "bunx tsc -p tsconfig.types.json",
    "build:js": "rollup -c",
    "lint": "yarn eslint --max-warnings=0 .",
    "fmt": "yarn eslint --max-warnings=0 --fix ."
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "rollup": "^4.53.3",
    "tslib": "^2.8.1",
    "typescript-eslint": "^8.53.0",
    "@rollup/plugin-json": "^6.1.0",
    "rollup-plugin-dts": "^6.3.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@opentelemetry/sdk-node": "^0.209.0"
  },
  "engines": {
    "node": ">=20"
  },
  "keywords": [
    "dagger",
    "opentelemetry"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dagger/dagger/tree/main/sdk/typescript/telemetry"
  },
  "bugs": {
    "url": "https://github.com/dagger/dagger/issues"
  },
  "homepage": "https://dagger.io"
}
